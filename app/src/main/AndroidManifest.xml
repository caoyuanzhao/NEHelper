<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.scse.nehelper">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".CommandSearch"/>
        <activity android:name=".PartitionResult"/>
        <activity android:name=".SubnetPartition"/>
        <activity android:name=".RouteSummarize"/>
        <activity android:name=".CorrelationDynamics"/>
        <activity android:name=".NewsDisplayActivity"/>
        <activity android:name=".CommandResult"/>
    </application>
</manifest>